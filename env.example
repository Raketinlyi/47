# Alchemy API Key (get from https://www.alchemy.com/)
# Single key is used across the app
NEXT_PUBLIC_ALCHEMY_API_KEY_1=your_alchemy_key_here
# Comma-separated list of server-side Alchemy keys used for rotation
ALCHEMY_KEYS=key1,key2,key3,key4,key5
# Public Monad RPC fallback used when all Alchemy keys fail
MONAD_PUBLIC_RPC=https://rpc.monad.xyz

# Subgraph endpoint (legacy "SUBGRAPH_URL" kept for backward compatibility)
NEXT_PUBLIC_SUBGRAPH_URL=https://api.studio.thegraph.com/query/your-workspace/your-subgraph
SUBGRAPH_URL=https://api.studio.thegraph.com/query/your-workspace/your-subgraph

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Security Configuration
NEXT_PUBLIC_SECURITY_ENABLED=true
NEXT_PUBLIC_RATE_LIMIT_ENABLED=true
NEXT_PUBLIC_CSRF_ENABLED=true

# Debug Mode (set to true for development)
NEXT_PUBLIC_DEBUG_SECURITY=false 

# Enable Trusted Types policies (optional; may affect some mobile browsers)
NEXT_PUBLIC_TRUSTED_TYPES_ENABLED=false

# Burn element toggle: 'crystal' (default) or 'fire'
NEXT_PUBLIC_BURN_ELEMENT=crystal

# Multicall3 address (default: 0xcA11bde05977b3631167028862bE2a173976CA11)
NEXT_PUBLIC_MULTICALL3_ADDRESS=0xcA11bde05977b3631167028862bE2a173976CA11

# === Breed quote API (server) ===
# OpenSea collection floor price
OPENSEA_API_KEY=your_opensea_key
OPENSEA_SLUG=crazyoctogon

# Signed quote signer (server-only)
PRICE_SIGNER_PK=0xYOUR_SIGNER_PRIVATE_KEY

# Monad RPC + core/reader contracts
MONAD_RPC_URL=https://rpc.monad.xyz
CORE_ADDRESS=0xYourCoreProxyAddress
READER_ADDRESS=0xYourReaderAddress

# Tokens + pools (mainnet)
TOKEN_OCTA=0xBB848dAC056e385d2f7c750eC839157dccf4cfF3
TOKEN_MON=0x3bd359C1119dA7Da1D913D1C4D2B7c461115433A
TOKEN_CRAA=0x4bb09F3e7b4D79920dF4A90852a7a1b9aAD4Ff8B
TOKEN_USDC=0x754704Bc059F8C67012fEd69BC8A327a5aafb603
PAIR_OCTA_MON=0xYourOCTAMONPool
PAIR_CRAA_USDC=0xYourCRAAUSDCPool
PAIR_OCTA_USDC=0x370678b17b2dd6c727604adf2669666d1af2fa26

# Sanity bounds vs on-chain Reader floor (bps of on-chain price)
MIN_FLOOR_BPS=5000
MAX_FLOOR_BPS=20000
